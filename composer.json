{
    "name": "sierratecnologia/boravel",
    "description": "Recursos para Desenvolvimento Complexo",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "boravel",
        "botman",
        "chatbot",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/borave/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/borave/issues",
        "source": "https://github.com/sierratecnologia/borave",
        "docs": "https://github.com/sierratecnologia/borave/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "privatejustice/socrates": "^0.4.0",
        "sierratecnologia/crypto": "^0.4.0",
        "sierratecnologia/muleta": "^0.4.0",
        "sierratecnologia/integrations": "^0.4.0",
        "sierratecnologia/transmissor": "^0.4.0",
        "sierratecnologia/locaravel": "^0.4.0",
        "sierratecnologia/laravel-support": "^0.4.0"
    },
    "suggest": {
        "sierratecnologia/payment-laravel": "Pagamento"
    },
    "autoload": {
        "psr-4": {
            "Boravel\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Boravel\\BoravelProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    },
    "require-dev": {
        "phpro/grumphp": "^1.3"
    }
}
