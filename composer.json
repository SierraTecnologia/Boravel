{
    "name": "sierratecnologia/boravel",
    "description": "Recursos para Desenvolvimento Complexo",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "boravel",
        "botman",
        "chatbot",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/borave/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/borave/issues",
        "source": "https://github.com/sierratecnologia/borave",
        "docs": "https://github.com/sierratecnologia/borave/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "privatejustice/socrates": "dev-master",
        "sierratecnologia/crypto": "dev-master",
        "sierratecnologia/muleta": "dev-master",
        "sierratecnologia/integrations": "dev-master",
        "sierratecnologia/transmissor": "dev-master",
        "sierratecnologia/locaravel": "dev-master",
        "sierratecnologia/laravel-support": "dev-master"
    },
    "suggest": {
        "sierratecnologia/payment-laravel": "Pagamento"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Boravel\\": "src/"
        }
    },
     "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "check-style": "phpcs -p --standard=PSR12 src --extensions=php",
        "fix-style": "phpcbf -p --standard=PSR12 src --extensions=php"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Boravel\\BoravelProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
